{
  "name": "electron-force",
  "version": "0.0.1",
  "description": "An Electron Wrapper on JSForce",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint --ignore-path .gitignore src",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wlm",
    "postinstall": "electron-builder install-app-deps",
    "test": "mocha"
  },
  "build": {
    "appId": "com.electron.electronForce",
    "productName": "ElectronForce",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "zip"
    },
    "win": {
      "target": "zip"
    },
    "linux": {
      "target": "zip"
    }
  },
  "keywords": [
    "Electron",
    "JSForce",
    "Salesforce"
  ],
  "author": "Aaron Crosman <acrosman@gmail.com> (https://spinningcode.org)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acrosman/electronForce"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "jquery": "^3.5.0",
    "jquery.json-viewer": "^1.4.0",
    "jsforce": "^1.9.3",
    "minimist": "^1.2.5",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-debug": "^2.2.0",
    "chai": "^4.2.0",
    "debug-menu": "^0.6.1",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0"
  }
}
